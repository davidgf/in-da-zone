{
  "manifest_version": 2,
  "name": "In Da Zone",
  "version": "1.0",
  "description": "",
  "icons": {
    "48": "static/icons/icon_48.png",
    "96": "static/icons/icon_96.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "16": "static/icons/icon_16.png",
      "32": "static/icons/icon_32.png",
      "48": "static/icons/icon_48.png",
      "64": "static/icons/icon_64.png",
      "96": "static/icons/icon_96.png"
    },
    "default_title": "In Da Zone",
    "default_popup": "popup/index.html"
  },
  "web_accessible_resources": [
    "static/redirect/redirect.html"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}