{
  "manifest_version": 2,
  "name": "In Da Zone",
  "version": "1.0",
  "description": "",
  "icons": {
    "48": "dist/static/icons/icon_48.png",
    "96": "dist/static/icons/icon_96.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "16": "dist/static/icons/icon_16.png",
      "32": "dist/static/icons/icon_32.png",
      "48": "dist/static/icons/icon_48.png",
      "64": "dist/static/icons/icon_64.png",
      "96": "dist/static/icons/icon_96.png"
    },
    "default_title": "In Da Zone",
    "default_popup": "dist/popup/index.html"
  },
  "web_accessible_resources": [
    "dist/static/redirect/redirect.html"
  ],
  "background": {
    "scripts": [
      "dist/background.js"
    ],
    "persistent": true
  }
}